{
	"name": "KBI_BIDYN_08_TEST",
	"properties": {
		"description": "Flusso che legge da Oracle e scrive su DB Sql di Staging area",
		"activities": [
			{
				"name": "BIDYN08-To-Station_08",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "OracleSource",
						"partitionOption": "None",
						"queryTimeout": "02:00:00"
					},
					"sink": {
						"type": "AzureSqlSink",
						"tableOption": {
							"value": "TRUNCATE TABLE [ntv].[DynInd_Station_08]",
							"type": "Expression"
						},
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "DATA_VENDITA",
									"type": "DateTime"
								},
								"sink": {
									"name": "SalesDate",
									"type": "DateTime",
									"physicalType": "date"
								}
							},
							{
								"source": {
									"name": "VENDUTO",
									"type": "Decimal"
								},
								"sink": {
									"name": "Amount",
									"type": "Decimal",
									"physicalType": "decimal",
									"scale": 2,
									"precision": 18
								}
							},
							{
								"source": {
									"name": "STAZIONE",
									"type": "String"
								},
								"sink": {
									"name": "Station",
									"type": "String",
									"physicalType": "nchar"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "DS_Kpi_BIDYN_08",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DynInd_Station_08",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Az_IntegrazioniBatch_Oracle_Test"
		},
		"annotations": []
	}
}