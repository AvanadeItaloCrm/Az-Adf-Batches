{
	"name": "Armando-Loop-001",
	"properties": {
		"activities": [
			{
				"name": "From Dynamics to Sql DB",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DynamicsSource",
						"query": "<fetch top=\"1\" >\n  <entity name=\"ntv_customercluster\" >\n    <attribute name=\"ntv_customerclusterid\" />\n    <filter>\n      <condition attribute=\"ntv_name\" operator=\"eq\" value=\"Base\" />\n    </filter>\n  </entity>\n</fetch>"
					},
					"sink": {
						"type": "AzureSqlSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "ntv_customerclusterid",
									"type": "Guid"
								},
								"sink": {
									"name": "ntv_clientid",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "Dynamics365Entity",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlTableArmandoTest",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": [],
		"lastPublishTime": "2021-09-27T18:02:33Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}