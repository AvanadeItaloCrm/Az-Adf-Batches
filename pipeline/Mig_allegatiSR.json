				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "\nSELECT B.SR_NUM+'_'+B.NOME_FILE+'.'+B.ESTENSIONE_FILE AS NOMEFILE\n  FROM [datav].[Metadata_crmdevdyn365data] A WITH (NOLOCK)\n\n  join [stg].[M_ALLEGATI] B WITH (NOLOCK)\n  ON A.FileName=CONCAT(CONCAT(B.NOME_FILE,'.'),B.ESTENSIONE_FILE)\n",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "Mig__Allegati",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
